{
  "description": "물류센터 구역 데이터 스키마",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "description": "구역 아이디",
      "example": "IB-01"
    },
    "name": {
      "type": "string",
      "description": "구역 이름",
      "example": "입고"
    },
    "lines": {
      "type": "integer",
      "description": "라인 개수",
      "example": 4
    },
    "length": {
      "type": "integer",
      "description": "라인 길이 (미터)",
      "example": 50
    },
    "sensors": {
      "type": "integer",
      "description": "센서 개수",
      "example": 40
    }
  },
  "required": ["id", "name", "lines", "length", "sensors"]
}
