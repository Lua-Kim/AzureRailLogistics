{
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.1",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {
              "containerogis": {
                "username": "containerogis",
                "password": "${REGISTRY_PASSWORD}",
                "address": "containerogis.azurecr.io"
              }
            }
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.5",
              "createOptions": ""
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.5",
              "createOptions": "{\"HostConfig\":{\"PortBindings\":{\"5671/tcp\":[{\"HostPort\":\"5671\"}],\"8883/tcp\":[{\"HostPort\":\"8883\"}],\"443/tcp\":[{\"HostPort\":\"443\"}]}}}"
            }
          }
        },
        "modules": {
          "postgres": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "postgres:15",
              "createOptions": "{\"Env\":[\"POSTGRES_DB=logistics\",\"POSTGRES_USER=admin\",\"POSTGRES_PASSWORD=admin123\"],\"HostConfig\":{\"Binds\":[\"postgres-data:/var/lib/postgresql/data\"],\"PortBindings\":{\"5432/tcp\":[{\"HostPort\":\"5432\"}]}}}"
            }
          },
          "logistics-backend": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "containerogis.azurecr.io/logistics-backend:latest",
              "createOptions": "{\"Env\":[\"DATABASE_URL=postgresql://admin:admin123@postgres:5432/logistics\",\"IOT_HUB_CONNECTION_STRING=${IOT_HUB_CONNECTION_STRING}\",\"EVENTHUB_CONNECTION_STRING=${EVENTHUB_CONNECTION_STRING}\",\"EVENTHUB_CONSUMER_GROUP=\\\$Default\"],\"HostConfig\":{\"PortBindings\":{\"8000/tcp\":[{\"HostPort\":\"8000\"}]}}}"
            }
          },
          "logistics-sensor-simulator": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "containerogis.azurecr.io/logistics-sensor-simulator:latest",
              "createOptions": "{\"Env\":[\"DATABASE_URL=postgresql://admin:admin123@postgres:5432/logistics\",\"IOT_HUB_CONNECTION_STRING=${IOT_HUB_CONNECTION_STRING}\"]}"
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.2",
        "routes": {
          "sensorToIoTHub": "FROM /messages/modules/logistics-sensor-simulator/outputs/* INTO $upstream"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    }
  }
}
